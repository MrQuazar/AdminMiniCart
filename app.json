{
  "expo": {
    "name": "Admin MiniCart",
    "slug": "AdminMiniCart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/miniAdminIcon.png",
    "splash": {
      "image": "./assets/AdminSplashScreen.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/miniAdminIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rcoe.adminminicart"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Mini Cart's Admin version for shopkeepers. Has options to check orders uploaded on the database and give the live status of orders.",
    "githubUrl": "https://github.com/MrQuazar/AdminMiniCart"
  }
}
